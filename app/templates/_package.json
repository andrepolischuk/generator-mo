{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "keywords": [
    ""
  ],
  "repository": "<%= githubUsername %>/<%= name %>",
  "dependencies": {
    <% if (cli) { %>"meow": "^3.3.0"<% } %>
  },
  "devDependencies": {
    "ava": "^0.2.0",
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chokidar-cli": "^1.1.0",
    "rimraf": "^2.4.3"
  },
  "main": "index.es5.js",<% if (cli) { %>
