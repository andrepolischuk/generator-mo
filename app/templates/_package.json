{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "keywords": [
    ""
  ],
  "repository": "<%= githubUsername %>/<%= name %>",
  "dependencies": {
    <% if (cli) { %>"meow": "^3.3.0"<% } %>
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "mocha": "^2.2.5"
  },
  "main": "index.es5.js",<% if (cli) { %>
  "bin": "cli.es5.js",<% } %>
  "scripts": {
    "test": "mocha --require babel/register",
    "transpile": "babel --stage 0 index.js > index.es5.js<% if (cli) { %> && babel --stage 0 cli.js > cli.es5.js<% } %>",
    "prepublish": "npm run transpile",
    "postpublish": "rm -rf *.es5.js"
  },
  "author": "<%= githubName %> <<%= githubEmail %>><% if (githubWebsite) { %> (<%= githubWebsite %>)<% } %>",
  "license": "MIT"
}
