{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "keywords": [
    ""
  ],
  "repository": "<%= githubUsername %>/<%= name %>",
  "dependencies": {
    <% if (cli) { %>"meow": "^3.3.0"<% } %>
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chokidar-cli": "^1.1.0",
    "rimraf": "^2.5.2"
  },
  "main": "index.es5.js",<% if (cli) { %>
  "bin": "cli.es5.js",<% } %>
  "scripts": {
    "test": "ava --require babel-register",
    "tdd": "chokidar \"*.js\" --ignore \"*.es5.js\" --command \"npm run test\" --initial",
    "prepublish": "babel index.js --out-file index.es5.js<% if (cli) { %> && babel cli.js --out-file cli.es5.js<% } %>",
    "postpublish": "rimraf index.es5.js<% if (cli) { %> cli.es5.js<% } %> && git push --follow-tags"
  },
  "author": "<%= githubName %> <<%= githubEmail %>><% if (githubWebsite) { %> (<%= githubWebsite %>)<% } %>",
  "license": "MIT"
}
