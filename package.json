{
  "name": "generator-mo",
  "version": "1.1.1",
  "description": "Yeoman generator for ES2015 node and browser modules",
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "scripts": {
    "pretest": "npm run prepublish",
    "test": "ava",
    "tdd": "chokidar \"*.js\" \"src/*.js\" --command \"npm run test\" --initial",
    "prepublish": "babel src --out-dir app",
    "postpublish": "rimraf app/index.js && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrepolischuk/generator-mo.git"
  },
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "es6",
    "ES2015",
    "node",
    "browser",
    "module"
  ],
  "author": "Andrey Polischuk <andre.polischuk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrepolischuk/generator-mo/issues"
  },
  "homepage": "https://github.com/andrepolischuk/generator-mo#readme",
  "dependencies": {
    "generator-babel": "^2.0.2",
    "generator-travis": "^1.2.4",
    "gh-user": "^2.1.0",
    "pinkie-promise": "^2.0.1",
    "to-camel-case": "^1.0.0",
    "yeoman-generator": "^0.22.6"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.6.0",
    "chokidar-cli": "^1.1.0",
    "npm-run-all": "^1.7.0",
    "pify": "^2.3.0",
    "rimraf": "^2.5.2",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.1.0"
  }
}
