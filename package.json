{
  "name": "generator-mo",
  "description": "Yeoman generator for ES2015 node and browser modules",
  "version": "1.0.0",
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "es6",
    "ES2015",
    "node",
    "browser",
    "module"
  ],
  "repository": "andrepolischuk/generator-mo",
  "dependencies": {
    "gh-user": "^1.0.0",
    "to-case": "^1.0.1",
    "yeoman-generator": "^0.21.1"
  },
  "devDependencies": {
    "ava": "^0.7.0",
    "babel-cli": "^6.3.17",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.3.13",
    "chokidar-cli": "^1.1.0",
    "npm-run-all": "^1.2.11",
    "pify": "^2.3.0",
    "rimraf": "^2.4.3"
  },
  "main": "app/index.js",
  "scripts": {
    "pretest": "npm run prepublish",
    "test": "ava",
    "tdd": "chokidar \"*.js\" --command \"npm run test\" --initial",
    "prepublish": "babel --presets es2015 --plugins add-module-exports src --out-dir app",
    "postpublish": "rimraf app/index.js && git push --follow-tags"
  },
  "author": "Andrey Polischuk <andre.polischuk@gmail.com>",
  "license": "MIT"
}
